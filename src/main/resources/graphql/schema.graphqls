type Book {
    id : ID!
    name: String
    rating: Rating
    authors: [Author]
}

type Author {
    id : ID!
    name:String
    surname:String
    age: String
}

enum Rating{
    FIVE, FOUR, THREE, TWO, ONE
}

type Query {
    allBooks : [Book]
    allAuthors : [Author]
}

type Mutation {
    createAuthor(name: String,surname:String,age:String):Author
}